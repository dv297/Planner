{
  "name": "planner",
  "version": "1.0.0",
  "description": "Web app to help plan projects and tasks",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next",
    "dev:logs": "next > server-logs.txt",
    "build": "npm run db:migrate && npm run build:db && next build",
    "build:db": "prisma generate",
    "build:docker": "docker build -t nextjs-docker .",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "cypress:headless": "CYPRESS_BASE_URL=http://localhost:3000  cypress run",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:wait-and-migrate": "wait-on http-get://localhost:5432 && npm run db:migrate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:backup": "cd database && docker exec -t postgres pg_dumpall -c -U postgres > dump_`date +%d-%m-%Y\"_\"%H_%M_%S`.sql && rm backup.sql ",
    "db:restore": "cat database/backup.sql | docker exec -i postgres psql -U root",
    "db:reset": "prisma migrate reset",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:open",
    "e2e:headless": "start-server-and-test dev:logs http://localhost:3000 cypress:headless",
    "format": "npm run format:code && npm run format:prisma",
    "format:code": "prettier --write .",
    "format:prisma": "prisma format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "check": "npm run format && npm run lint && npm run build && npm run e2e:headless",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
